
@{
    ViewBag.Title = "EditDeleteUserList";
}

<h2 class="bro-text">List of Bros</h2>

<table id="candle-table" class="table border-dark">
    
    <tr>
        <th>User Name</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone Number</th>
        <th>Email</th>
        <th>State</th>
    </tr>

    @foreach (var user in ViewBag.UserList)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            @if (user.PhoneNumber != null)
            {
                <td>@user.PhoneNumber</td>
            }
            <td>@user.Email</td>
            <td>@user.State</td>
        </tr>
    }
    

</table>